{
  "name": "@youcan/ui",
  "version": "0.1.12",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js"
    },
    "./style": {
      "require": "./dist/index.css",
      "import": "./dist/index.css"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "rimraf dist && vue-tsc --declaration --emitDeclarationOnly --project tsconfig.build.json && vite build",
    "story:dev": "histoire dev --port 3452",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "up": "taze major",
    "release": "pnpm build && pnpm lint:fix && bumpp --commit --push --tag && pnpm publish --access public"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.11.9",
    "@iconify-json/line-md": "^1.1.20",
    "@iconify-json/tabler": "^1.1.52",
    "@sidi3li/eslint-config": "^1.0.10",
    "@types/node": "18.7.15",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vueuse/core": "^9.6.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "histoire": "^0.11.9",
    "rimraf": "^3.0.2",
    "sass": "1.54.8",
    "taze": "^0.8.4",
    "typescript": "4.8.2",
    "unocss": "^0.47.6",
    "vite": "^4.0.1",
    "vue": "^3.2.45",
    "vue-tsc": "1.0.9"
  }
}
