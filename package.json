{
  "name": "@youcan/ui",
  "version": "0.1.0",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "rimraf dist && vue-tsc --declaration --emitDeclarationOnly --project tsconfig.build.json && vite build",
    "story:dev": "histoire dev --port 3452",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "pnpm lint:fix && bumpp --commit --push --tag && pnpm publish"
  },
  "peerDependencies": {
    "vue": "^2.7.13"
  },
  "dependencies": {
    "@vueuse/core": "^9.2.0"
  },
  "devDependencies": {
    "@histoire/plugin-vue2": "^0.11.6",
    "@iconify-json/tabler": "^1.1.30",
    "@sidi3li/eslint-config": "^1.0.10",
    "@types/node": "18.7.15",
    "@vitejs/plugin-vue2": "^2.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.17.0",
    "histoire": "^0.11.6",
    "rimraf": "^3.0.2",
    "sass": "1.54.8",
    "typescript": "4.8.2",
    "unocss": "^0.46.3",
    "vite": "^3.2.3",
    "vue": "^2.7.13",
    "vue-tsc": "0.40.7"
  }
}
