{
  "name": "@youcan/ui-icons",
  "type": "module",
  "version": "2.6.1-beta.1",
  "description": "YouCan icon set in Iconify JSON format",
  "author": "YouCan <tech@youcan.shop> (https://github.com/youcan-shop/)",
  "license": "MIT",
  "homepage": "https://github.com/youcan-shop/youcan-ui",
  "keywords": [
    "youcan",
    "ui",
    "icons"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "require": {
        "default": "./index.js",
        "types": "./index.d.ts"
      },
      "import": {
        "default": "./index.mjs",
        "types": "./index.d.ts"
      }
    },
    "./icons.json": "./icons.json",
    "./info.json": "./info.json"
  },
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "iconSet": {
    "icons": "icons.json",
    "info": "info.json"
  },
  "scripts": {
    "build": "node tooling/transpile.js",
    "release": "pnpm publish --access public"
  },
  "dependencies": {
    "@iconify/types": "*"
  },
  "devDependencies": {
    "@iconify/tools": "^2.1.2",
    "@iconify/utils": "^2.0.9",
    "fs-extra": "^11.1.0"
  }
}
