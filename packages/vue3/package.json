{
  "name": "@youcan/ui-vue3",
  "version": "2.1.3",
  "description": "YouCan Vue3 components library",
  "author": "YouCan <tech@youcan.shop> (https://github.com/youcan-shop/)",
  "license": "MIT",
  "homepage": "https://github.com/youcan-shop/youcan-ui",
  "keywords": [
    "youcan",
    "ui",
    "vue3",
    "components"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js"
    },
    "./style": {
      "require": "./dist/index.css",
      "import": "./dist/index.css"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "story:dev": "storybook dev -p 6006",
    "story:build": "storybook build",
    "components:dev": "vite dev",
    "components:build": "rimraf dist && vue-tsc --declaration --emitDeclarationOnly --project tsconfig.build.json && vite build",
    "dev": "pnpm story:dev & pnpm components:dev",
    "build": "pnpm components:build & pnpm story:build",
    "release": "pnpm publish --access public"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@iconify-json/line-md": "^1.1.20",
    "@iconify-json/tabler": "^1.1.53",
    "@storybook/addon-essentials": "7.0.0-beta.14",
    "@storybook/addon-interactions": "7.0.0-beta.14",
    "@storybook/addon-links": "7.0.0-beta.14",
    "@storybook/blocks": "7.0.0-beta.14",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "7.0.0-beta.14",
    "@storybook/vue3-vite": "7.0.0-beta.14",
    "@types/node": "18.7.15",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/core": "^9.9.0",
    "@youcan/ui-core": "*",
    "@youcan/ui-icons": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "1.54.8",
    "storybook": "7.0.0-beta.14",
    "typescript": "4.8.2",
    "unocss": "^0.47.6",
    "vite": "^4.0.3",
    "vue": "^3.2.45",
    "vue-tsc": "1.0.9"
  },
  "dependencies": {
    "@tiptap/extension-color": "2.0.0-beta.213",
    "@tiptap/extension-highlight": "2.0.0-beta.213",
    "@tiptap/extension-horizontal-rule": "2.0.0-beta.209",
    "@tiptap/extension-image": "2.0.0-beta.213",
    "@tiptap/extension-table": "2.0.0-beta.213",
    "@tiptap/extension-table-cell": "2.0.0-beta.213",
    "@tiptap/extension-table-header": "2.0.0-beta.213",
    "@tiptap/extension-table-row": "2.0.0-beta.213",
    "@tiptap/extension-text-align": "^2.0.0-beta.213",
    "@tiptap/extension-text-style": "2.0.0-beta.213",
    "@tiptap/extension-underline": "2.0.0-beta.209",
    "@tiptap/starter-kit": "2.0.0-beta.209",
    "@tiptap/vue-3": "2.0.0-beta.209",
    "prosemirror-commands": "^1.5.0",
    "prosemirror-dropcursor": "^1.6.1",
    "prosemirror-gapcursor": "^1.3.1",
    "prosemirror-history": "^1.3.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.19.0",
    "prosemirror-schema-list": "^1.2.2",
    "prosemirror-state": "^1.4.2",
    "prosemirror-transform": "^1.7.1",
    "prosemirror-view": "^1.30.0"
  }
}
