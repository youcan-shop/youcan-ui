{
  "name": "@youcan/ui-vue3",
  "version": "1.0.0",
  "description": "YouCan Vue3 components library",
  "author": "YouCan <tech@youcan.shop> (https://github.com/youcan-shop/)",
  "license": "MIT",
  "homepage": "https://github.com/youcan-shop/youcan-ui",
  "keywords": [
    "youcan",
    "ui",
    "vue3",
    "components"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js"
    },
    "./style": {
      "require": "./dist/index.css",
      "import": "./dist/index.css"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "rimraf dist && vue-tsc --declaration --emitDeclarationOnly --project tsconfig.build.json && vite build"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@iconify-json/line-md": "^1.1.20",
    "@iconify-json/tabler": "^1.1.53",
    "@types/node": "18.7.15",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/core": "^9.9.0",
    "rimraf": "^3.0.2",
    "sass": "1.54.8",
    "typescript": "4.8.2",
    "unocss": "^0.47.6",
    "vite": "^4.0.3",
    "vue": "^3.2.45",
    "vue-tsc": "1.0.9"
  }
}
